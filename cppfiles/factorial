#include <iostream>
using namespace std;
int main() {
  int input;
  cout<<"Enter a number: ";
  cin>>input;
  for(int i=1;i<=input;i++){
    int factorial=1;
    cout<<i<<"!=";
     for (int j=1;j<=i;j++){
        factorial=factorial*j;
        if(i>j){
        cout<<j<<"x";
        }
        else 
        cout<<j<<"="; 
     }
     cout<<factorial<<endl;
  }
}
