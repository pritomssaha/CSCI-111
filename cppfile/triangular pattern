#include <iostream>
using namespace std;
int main() {
  int input;
  cout<<"Enter a number: ";
  cin>>input;
  for(int i=1;i<=input;i++){
     
     if (i%2!=0){
       for (int j=1;j<=i;j++){
         cout<<"1";
       }
     }
    
     else if (i%2==0){
       for (int k=1;k<=i;k++){
         cout<<"0";
       }
     }
      cout<<endl;    
  }
}
